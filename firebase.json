{
  "firestore": {
    "database": "(default)",
    "location": "asia-south1",
    "rules": "firestore.rules",
    "indexes": "firestore.indexes.json"
  },
  "functions": {
    "source": "functions",
    "runtime": "python312",
    "region": "asia-south1"
  },
  "hosting": {
    "public": "frontend/out",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "headers": [
      {
        "source": "/_next/static/**/*.js",
        "headers": [
          {
            "key": "Content-Type",
            "value": "application/javascript"
          },
          {
            "key": "Cache-Control",
            "value": "public, max-age=31536000, immutable"
          }
        ]
      },
      {
        "source": "/_next/static/**/*.css",
        "headers": [
          {
            "key": "Content-Type",
            "value": "text/css"
          },
          {
            "key": "Cache-Control",
            "value": "public, max-age=31536000, immutable"
          }
        ]
      },
      {
        "source": "**",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "no-cache, no-store, must-revalidate"
          }
        ]
      }
    ],
    "rewrites": [
      {
        "source": "/_next/static/**",
        "destination": "/_next/static/**"
      },
      {
        "source": "/interview/**",
        "destination": "/interview/index.html"
      },
      {
        "source": "/dashboard/memo",
        "destination": "/dashboard/memo.html"
      },
      {
        "source": "/dashboard/billing",
        "destination": "/dashboard/billing.html"
      },
      {
        "source": "/dashboard/create-room",
        "destination": "/dashboard/create-room.html"
      },
      {
        "source": "/dashboard/diligence",
        "destination": "/dashboard/diligence.html"
      },
      {
        "source": "/dashboard/interview",
        "destination": "/dashboard/interview.html"
      },
      {
        "source": "/dashboard/messages",
        "destination": "/dashboard/messages.html"
      },
      {
        "source": "/dashboard/settings",
        "destination": "/dashboard/settings.html"
      },
      {
        "source": "/dashboard/support",
        "destination": "/dashboard/support.html"
      },
      {
        "source": "/dashboard/**",
        "destination": "/dashboard.html"
      },
      {
        "source": "/founder/dashboard/datarooms",
        "destination": "/founder/dashboard/datarooms.html"
      },
      {
        "source": "/founder/dashboard/documents",
        "destination": "/founder/dashboard/documents.html"
      },
      {
        "source": "/founder/dashboard/feedback",
        "destination": "/founder/dashboard/feedback.html"
      },
      {
        "source": "/founder/dashboard/investors",
        "destination": "/founder/dashboard/investors.html"
      },
      {
        "source": "/founder/dashboard/messages",
        "destination": "/founder/dashboard/messages.html"
      },
      {
        "source": "/founder/dashboard/profile",
        "destination": "/founder/dashboard/profile.html"
      },
      {
        "source": "/founder/dashboard/settings",
        "destination": "/founder/dashboard/settings.html"
      },
      {
        "source": "/founder/dashboard/**",
        "destination": "/founder/dashboard.html"
      },
            {
              "source": "/admin/memo-details/**",
              "destination": "/admin/memo-details/index.html"
            },
            {
              "source": "/admin/memos/*",
              "destination": "/admin/memos/[id]/index.html"
            },
            {
              "source": "/admin/**",
              "destination": "/admin/index.html"
            },
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  },
  "storage": {
    "rules": "storage.rules",
    "bucket": "veritas-472301.firebasestorage.app"
  }
}
